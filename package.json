{
  "name": "space-rocket",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "date-fns": "^1.29.0",
    "node-sass": "^4.9.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-flexbox-grid": "https://github.com/roylee0704/react-flexbox-grid.git#master",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build": "yarn sass:build && react-scripts build",
    "eject": "react-scripts eject",
    "sass:watch": "node-sass -w src/styles/scss -o src/styles/css",
    "sass:build": "node-sass src/styles/scss -o src/styles/css --source-map true",
    "start": "yarn run sass:build && react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "postinstall": "(cd node_modules/react-flexbox-grid && yarn install && yarn run build)"
  }
}
